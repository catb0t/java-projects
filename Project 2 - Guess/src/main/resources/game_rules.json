{
  "___COPYRIGHT": [ "Copyright (c) 2022 Cat Stevens. All rights reserved." ],
  "stringsFile": [ "guess_strings" ],
  "maxTries": 6,
  "canPlayAgain": true,
  "validInputPattern": "^1?[0-9]|20$",
  "isInputPatternFullMatch": true,
  "define_names": [
    { "name": "$min_target", "value": 1, "type": "int" },
    { "name": "$max_target", "value": 20, "type": "int" },
    {
      "name": "$guess_target",
      "type": "listmap",
      "value": [
        { "choice": [ "weighted_choice" ] },
        { "static_value": [ "randInt", "1", "20" ] },
        { "deferred_value": [ "randInt", "1", "20" ] }
      ]
    }
  ],

  "eachIteration": [
    [ ["if"], [ "<", "$guess_target", "$prompt_input" ], ["@guessHigherMessage"] ],
    [ ["if"], [ ">", "$guess_target", "$prompt_input" ], ["@guessLowerMessage"] ]
  ],

  "winCondition": [ "equal", "$guess_target", "$prompt_input" ]
}